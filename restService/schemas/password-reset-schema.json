{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Reset Password Schema",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 6
    }
  },
  "required": ["email", "password"]
}
